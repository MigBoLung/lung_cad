dll = "/mnt/WORK/lungcad_work/LUNG_C/build/libmigfpr_2_zernike_mip.so"           ; fpr2 dll to use

svm_model_file = "/mnt/WORK/lungcad_work/data/models/LIDC_NEW_all_al3_w0_0_4.model"  ; libsvm format model file
svm_norm_file  = "/mnt/WORK/lungcad_work/data/scales/LIDC_NEW_all_al3_zer_1to8.norm"                       ; feature normalization parameters

